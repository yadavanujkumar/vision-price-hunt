# Vision Price Hunt - Real-time Scraping Configuration

# Set to false to disable real-time scraping and use mock data only
ENABLE_REAL_SCRAPING=true

# Whether to fallback to mock data when real scraping fails  
SCRAPING_FALLBACK_TO_MOCK=true

# Maximum number of products to extract per source
MAX_PRODUCTS_PER_SOURCE=5

# Request timeout in seconds
REQUEST_TIMEOUT=30

# Maximum number of retry attempts for failed requests
MAX_RETRIES=3

# Minimum delay between requests (in seconds)
SCRAPING_DELAY_MIN=1.0

# Maximum delay between requests (in seconds)  
SCRAPING_DELAY_MAX=3.0